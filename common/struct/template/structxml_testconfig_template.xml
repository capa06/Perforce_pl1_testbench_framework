<testconfig>	<opts>		<!-- Log level choices 'CRITICAL', 'ERROR', 'WARNING', 'INFO', 'DEBUG' -->	    <field name='log'></field>	    <!-- choices 'LTE','WCMDA', 'GSM' -->			<field name='rat'></field>			<!-- IP address of CMW500 -->		<field name='cmwip'></field>			<!-- CMW500 tag-->		<field name='cmwname'></field>			<!-- Modem Control Interface, {'AT', 'STDIN', 'ADB', "//<url>" } -->        		<field name='ctrlif'></field>			<!-- XML testplan file-->		<field name='testplan'></field>		<!-- enable/disable USIM emulator -->        		<field name='usimemu'></field>						<!-- enable/disable power supply -->		<field name='psu'></field>			<!-- enable/disable power measurements -->		<field name='pwrmeas'></field>			<!-- IP address of LAN-GPIB switch for power supply -->		<!-- only applicable if power supply or power measurements are enabled -->		<field name='psugwip'></field>			<!-- GPIB address of power supply connected to LAN-GPIB switch -->				<field name='psugpib'></field>		<!-- Enable Switched Cabinet Data Unit -->						<field name='enable_scdu'></field>		<!-- SCDU IP address -->							    <field name='scdu_ip'></field>		<!-- SCDU mapped modem port -->						  		<field name='scdu_port'></field>		<!-- SCDU userID -->						  		<field name='scdu_uid'></field>		<!-- SCDU password -->						  		<field name='scdu_pwd'></field>			<!-- enable/disable of modem logging -->		<field name='msglog'></field>			<!-- Flag [0/1] to indicate if remote Database needs updating or not -->				<field name='remoteDB'></field>			<!-- Remote DB host / ip address -->				<field name='remoteDBhost'></field>			<!-- Remote DB User ID -->				<field name='remoteDBuid'></field>			<!-- Remote DB Password (enc) -->				<field name='remoteDBpwd'></field>			<!-- Remote DB Name -->				<field name='remoteDBname'></field>		</opts></testconfig><!-- *******************************************************************************************--><!--                       