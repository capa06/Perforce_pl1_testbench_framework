<cm500_config>	<common>		<!-- routing configuration for SISO scenario -->		<field name='routing_siso'>"RF1C,RX1,RF1C,TX1"</field>		<!-- routing configuration for MIMO 2x2 scenario {'RF1C,RX1,RF1C,TX1,RF3C,TX2', 'RF1C,RX1,RF1C,TX1,RF2C,TX2'} -->				<field name='routing_mimo2x2'></field>			<!-- security milenage -->		<field name='milenage'></field>		<!-- security IALgorithm -->		<field name='ialgorithm'>"</field>		<field name='cell_srs'></field>		<!-- enable/disable advanced settings for PRACH -->		<field name='enable_ulpwrctrl_eas'></field>		<!-- open loop target power dBm-->		<field name='olnpwr'></field>		<!-- closed loop target power dBm-->		<field name='tcp_cltrpwr'></field>		<!-- select tpc type: closed loop/open loop-->		<field name='tcp_set'></field>		<!-- uplink max power dBm-->		<field name='pmax'></field>		<!-- open loop target power dBm for advanced settings-->		<field name='eas_olnpwr'></field>		<!-- closed loop target power dBm for advanced settings-->		<field name='eas_tcp_cltrpwr'></field>		<!-- select tpc type: closed loop/open loop for advanced settings-->		<field name='eas_tcp_set'></field>		<!-- uplink max power dBm for advanced settings-->		<field name='eas_pmax'></field>		<field name='eas_rspower'></field>		<field name='eas_pirpower'></field>		<field name='eas_pnpusch'></field>		<field name='eas_pcalpha'></field>		<!-- Connection -->		<field name='ghopping'></field>		<field name='uecat'></field>		<field name='uecat_reported'></field>		<field name='dpcycle'></field>				<field name='aseission'></field>		<field name='fcoefficient'></field>		<!-- Connection TEST mode / Data Application mode -->		<field name='ctype'></field>		<field name='tmode'></field>		<field name='rlcmode'></field>		<!-- RRCReconfig/SIBReconfig-->		<field name='sibreconf'></field>		<!-- Keep RRC config-->		<field name='krrc'></field>		<!-- MAC -->		<field name='dlpadding'></field>		<field name='dleinsertion'></field>		<field name='sdnspco'></field>					</common>	<pcc>		<!-- external attenuation -->				<field name='eatt_in1'></field>			<field name='eatt_out1'></field>			<field name='eatt_out2'></field>			<!-- downlink power levels -->		<field name='pss_poffest'></field>			<field name='sss_poffest'></field>			<field name='pbch_poffest'></field>			<field name='pcfich_poffest'></field>			<field name='phich_poffest'></field>			<field name='pdcch_poffest'></field>		<field name='ocng'></field>			<field name='cellid'></field>				<field name='cqi_index'></field>							</pcc>	<scc>		<!-- external attenuation -->				<field name='eatt_in1'>0</field>			<field name='eatt_out1'>0</field>			<field name='eatt_out2'>0</field>			<!-- downlink power levels -->		<field name='pss_poffest'>-3</field>			<field name='sss_poffest'>-3</field>			<field name='pbch_poffest'>0</field>			<field name='pcfich_poffest'>0</field>			<field name='phich_poffest'>0</field>			<field name='pdcch_poffest'>-3</field>		<field name='ocng'>"ON"</field>			<field name='cellid'>1</field>		<field name='cqi_index'>0</field>				<field name='scc_amode'>"AUTO"</field>			</scc></cm500_config>