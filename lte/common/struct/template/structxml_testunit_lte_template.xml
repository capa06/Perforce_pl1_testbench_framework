<testunit>
	<common>
		<!-- Test ID -->
		<field name='testid' ></field>
		<!-- RAT : {'LTE_FDD', 'LTE_TDD', 'LTE_FDD_CA', 'LTE_TDD_CA' -->
		<field name='rat' ></field>
		<!-- Test type, used for selecting the test procedure. Naming convention: <rat>_periodicity>_<descr>
		    {'LTE_FDD_PERCL_BLER',            'LTE_TDD_PERCL_BLER',             'LTE_FDD_CA_PERCL_BLER',                 
	         'LTE_FDD_PERCL_BLER_FADING',     'LTE_TDD_PERCL_BLER_FADING',      'LTE_FDD_CA_PERCL_BLER_FADING',
	         'LTE_FDD_NIGHTLY_BLER',          'LTE_TDD_NIGHTLY_BLER',           'LTE_FDD_CA_NIGHTLY_BLER',              
	         'LTE_FDD_NIGHTLY_BLER_FADING',   'LTE_TDD_NIGHTLY_BLER_FADING',    'LTE_FDD_CA_NIGHTLY_BLER_FADING',      
	         'LTE_FDD_WEEKLY_BLER',           'LTE_TDD_WEEKLY_BLER',            'LTE_FDD_CA_WEEKLY_BLER',
	         'LTE_FDD_WEEKLY_BLER_FADING',    'LTE_TDD_WEEKLY_BLER_FADING',     'LTE_FDD_CA_WEEKLY_BLER_FADING',        
	         'LTE_FDD_MONTHLY_BLER',          'LTE_TDD_MONTHLY_BLER',           'LTE_FDD_CA_MONTHLY_BLER',
	         'LTE_FDD_MONTHLY_BLER_FADING',   'LTE_TDD_MONTHLY_BLER_FADING',    'LTE_FDD_CA_MONTHLY_BLER_FADING',
	         'LTE_FDD_CUSTOM_BLER',           'LTE_TDD_CUSTOM_BLER',            'LTE_FDD_CA_CUSTOM_BLER',               
	         'LTE_FDD_CUSTOM_BLER_FADING',    'LTE_TDD_CUSTOM_BLER_FADING',     'LTE_FDD_CA_CUSTOM_BLER_FADING',               
	         'LTE_FDD_CUSTOM_INTRA_HHO',      'LTE_TDD_CUSTOM_INTRA_HHO',       'LTE_FDD_CA_CUSTOM_INTRA_HHO',
	         'LTE_FDD_CUSTOM_TA_FADING',      'LTE_TDD_CUSTOM_TA_FADING',       'LTE_FDD_CA_CUSTOM_TA_FADING'} -->
		<field name='testtype' ></field>	
		<!-- Test description -->
		<field name='descr' ></field>
	</common>
	<pcc>
		<!-- Duplex Mode, {FDD, TDD} -->
		<field name='dmode' ></field>
		<!-- DL/UL configuration -->
		<field name='dlulconf' ></field>
		<!-- SS configuration -->
		<field name='ssconf' ></field>
		<!-- Bandwidth MHz, {1.4, 3, 5, 10, 15, 20} MHz -->
		<field name='bwmhz' ></field>
		<!-- RF band -->
		<field name='rfband' ></field>
		<!-- EARFCN(RF band) -->
		<field name='earfcn' ></field>
		<!-- CP duration {'NORM', 'EXT'} -->
		<field name='cp' ></field>
		<!-- Supported Transmission Mode per BW { 1, 2, 3, 4 } -->
		<field name='tm' ></field>
		<!-- Supported TX antennas per TM { 1, 2, 3, 4 } -->
		<field name='txants' ></field>
		<!-- PMI configurations -->
		<field name='pmi' ></field>
		<!-- RSEPRE [dBm/15KHz] -->	
		<field name='rsepre' ></field>
		<!-- PA [dB] { 0, -3, -6 }-->	
		<field name='pa' ></field>
		<!-- pB [dB] { 0, 1, 2, 3 }-->	
		<field name='pb' ></field>
		<!-- Channel propagation type -->
		<field name='chtype' ></field>
		<!-- SNR [dB] {-3...33}-->		
		<field name='snr' ></field>
		<!-- Doppler Shift Hz -->
		<field name='doppler' ></field>
		<!-- CMW500 scheduler type { 'UDCH', 'UDTT', 'CQI'} -->	
		<field name='schedtype' ></field>
		<!-- Downlink HARQ -->	
		<field name='harq' ></field>
		<!-- DLMCS per BW -->	
		<field name='dlmcs'     ></field>
		<!-- DLNPRB per BW    -->	
		<field name='dlnprb'    ></field>
		<!-- DLRBSTART per BW -->	
		<field name='dlrbstart' ></field>
		<!-- ULMCS per BW     -->	
		<field name='ulmcs'     ></field>
		<!-- ULNPRB per BW    -->	
		<field name='ulnprb'    ></field>
		<!-- ULRBSTART per BW -->	
		<field name='ulrbstart' ></field>
	</pcc>
	<scc>
		<!-- Duplex Mode, {FDD, TDD} -->
		<field name='dmode' ></field>
		<!-- DL/UL configuration -->
		<field name='dlulconf' ></field>
		<!-- SS configuration -->
		<field name='ssconf' ></field>
		<!-- Bandwidth MHz, {1.4, 3, 5, 10, 15, 20} MHz -->
		<field name='bwmhz' ></field>
		<!-- RF band -->
		<field name='rfband' ></field>
		<!-- EARFCN(RF band) -->
		<field name='earfcn' ></field>
		<!-- CP duration {'NORM', 'EXT'} -->
		<field name='cp' ></field>
		<!-- Supported Transmission Mode per BW { 1, 2, 3, 4 } -->
		<field name='tm' ></field>
		<!-- Supported TX antennas per TM { 1, 2, 3, 4 } -->
		<field name='txants' ></field>
		<!-- PMI configurations -->
		<field name='pmi' ></field>
		<!-- RSEPRE [dBm/15KHz] -->	
		<field name='rsepre' ></field>
		<!-- PA [dB] { 0, -3, -6 }-->	
		<field name='pa' ></field>
		<!-- pB [dB] { 0, 1, 2, 3 }-->	
		<field name='pb' ></field>
		<!-- Channel propagation type -->
		<field name='chtype' ></field>
		<!-- SNR [dB] {-3...33}-->		
		<field name='snr' ></field>
		<!-- Doppler Shift Hz -->
		<field name='doppler' ></field>
		<!-- CMW500 scheduler type { 'UDCH', 'UDTT', 'CQI'} -->	
		<field name='schedtype' ></field>
		<!-- Downlink HARQ -->	
		<field name='harq' ></field>
		<!-- DLMCS per BW -->	
		<field name='dlmcs'     ></field>
		<!-- DLNPRB per BW    -->	
		<field name='dlnprb'    ></field>
		<!-- DLRBSTART per BW -->	
		<field name='dlrbstart' ></field>
		<!-- ULMCS per BW     -->	
		<field name='ulmcs'     ></field>
		<!-- ULNPRB per BW    -->	
		<field name='ulnprb'    ></field>
		<!-- ULRBSTART per BW -->	
		<field name='ulrbstart' ></field>
	</scc>
</testunit>

